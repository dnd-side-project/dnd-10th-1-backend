{
  "name": "create-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "cm": "cz",
    "generate": "prisma generate",
    "studio": "dotenv -e .env.local -- prisma studio",
    "prepare": "is-ci || husky install",
    "local:build": "dotenv -e .env.local nest build",
    "local:start": "dotenv -e .env.local nest start --watch",
    "local:migrate": "dotenv -e .env.local prisma migrate dev",
    "local:seed": "dotenv -e .env.local prisma db seed",
    "dev:build:": "dotenv -e .env.dev nest build",
    "dev:start": "dotenv -e .env.dev nest start --watch",
    "dev:migrate": "dotenv -e .env.dev prisma migrate dev",
    "dev:seed": "dotenv -e .env.dev prisma db seed",
    "prod:build": "dotenv -e .env.prod nest build",
    "prod:start": "dotenv -e .env.prod nest start --watch",
    "prod:migrate": "dotenv -e .env.prod prisma migrate dev",
    "prod:seed": "dotenv -e .env.prod prisma db seed",
    "start": "node dist/src/main",
    "format": "prettier --write \"src/**/*.*.ts\" ",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:seed": "yarn local:seed",
    "db:migrate": "dotenv -e .env.local prisma migrate dev ",
    "db:clean": "dotenv -e .env.local -- prisma db push --force-reset",
    "db:start": "yarn generate && docker-compose --env-file .env.local -f docker-compose.local.yaml up -d",
    "db:setup": "yarn db:start && yarn db:clean && yarn db:seed",
    "docker:stop:container": "docker rm -f $(docker ps -qa)",
    "docker:remove:image": "docker rmi $(docker ps -qa)"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.495.0",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/websockets": "^10.3.0",
    "@prisma/client": "^5.8.1",
    "aws-sdk": "^2.1539.0",
    "bcrypt": "^5.1.1",
    "builder-pattern": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-kakao": "^1.0.1",
    "prisma": "^5.8.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/testing": "^10.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "20.11.5",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-kakao": "^1.0.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "axios": "^1.6.5",
    "chalk": "^5.3.0",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "console-table-printer": "^2.12.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gradient-string": "^2.0.2",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "29.7.0",
    "jira-prepare-commit-msg": "^1.7.2",
    "lint-staged": "^15.2.0",
    "mime-types": "^2.1.35",
    "nest-winston": "^1.9.4",
    "passport-facebook": "^3.0.0",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./test",
    "testRegex": [
      ".*\\.spec\\.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "jira-prepare-commit-msg"
    }
  }
}